{
    "manifest_version": 2,
    "name": "Image Preview",
    "version": "1.0",
    "description": "Extracts and opens image URLs from preview divs",
    "icons": {
      "48": "icon48.png"
    },
    "browser_action": {
      "default_icon": "icon48.png",
      "default_title": "Image Preview",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }
  